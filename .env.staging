# Staging Environment Variables for ROKO Network Marketing Site

NODE_ENV=production
VITE_BUILD_TARGET=staging
VITE_BUILD_ENV=staging

# ROKO Network Staging API
VITE_RPC_URL=https://staging-rpc.roko.network
VITE_WS_URL=wss://staging-ws.roko.network
VITE_API_BASE_URL=https://staging-api.roko.network
VITE_API_TIMEOUT=15000

# Blockchain Configuration - Testnet
VITE_CHAIN_ID=1338
VITE_NETWORK_NAME=ROKO Testnet
VITE_NETWORK_CURRENCY=tROKO
VITE_ETHERSCAN_API_KEY=YourEtherscanAPIKeyHere
VITE_ROKO_TOKEN_ADDRESS=0x6f222e04f6c53cc688ffb0abe7206aac66a8ff98

# Feature Flags - Staging (more permissive for testing)
VITE_ENABLE_WEB3=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PWA=true
VITE_ENABLE_3D_FEATURES=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_LIVE_STATS=true
VITE_ENABLE_MOCK_API=true

# Performance Configuration
VITE_CHUNK_SIZE_WARNING_LIMIT=1000
VITE_ENABLE_BUNDLE_ANALYZER=true
VITE_ENABLE_CACHE=false
VITE_CACHE_TTL=300

# CDN Configuration
VITE_CDN_URL=https://staging-cdn.roko.network
VITE_ASSETS_URL=https://staging-assets.roko.network

# Security Configuration
VITE_ENABLE_CSP=true
VITE_ENABLE_SECURITY_HEADERS=true
VITE_ALLOWED_ORIGINS=https://staging.roko.network,https://preview.roko.network

# Social Media Integration
VITE_TWITTER_HANDLE=@ROKONetwork
VITE_DISCORD_INVITE=https://discord.gg/roko
VITE_TELEGRAM_CHANNEL=https://t.me/rokonetwork
VITE_GITHUB_ORG=https://github.com/roko-network

# Contact Information
VITE_CONTACT_EMAIL=staging@roko.network
VITE_SUPPORT_EMAIL=staging-support@roko.network

# Branding
VITE_BRAND_NAME=ROKO Network (Staging)
VITE_BRAND_COLOR=#00d4aa
VITE_BRAND_LOGO=/logo.svg

# Staging URLs
VITE_PUBLIC_URL=https://staging.roko.network
DEPLOY_URL=https://staging.roko.network